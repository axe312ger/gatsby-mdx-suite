{
  "name": "gatsby-starter-mdx-suite-contentful",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.6.2",
  "author": "Benedikt RÃ¶tsch <info@benedikt.roetsch.de>",
  "scripts": {
    "build": "npx --node-arg \"-r esm\" gatsby build",
    "develop": "GATSBY_CONTENTFUL_OFFLINE=true nodemon -x 'npx --node-arg \"-r esm\" gatsby develop' -w './*.js'",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "format": "prettier --write '{./**/*.{js,jsx},*.{js,json}}'",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axe312ger/gatsby-suite-mdx"
  },
  "keywords": [
    "gatsby",
    "starter",
    "mdx"
  ],
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@gatsby-mdx-suite/contexts": "^0.0.7",
    "@gatsby-mdx-suite/helpers": "^0.2.4",
    "@gatsby-mdx-suite/i18n": "^0.2.2",
    "@gatsby-mdx-suite/mdx-boxes": "^0.3.1",
    "@gatsby-mdx-suite/mdx-copy": "^0.2.0",
    "@gatsby-mdx-suite/mdx-image": "^0.2.0",
    "@gatsby-mdx-suite/mdx-layout": "^0.5.0",
    "@gatsby-mdx-suite/mdx-link": "^0.2.0",
    "@gatsby-mdx-suite/mdx-timeline": "^0.2.0",
    "@gatsby-mdx-suite/mdx-video": "^0.2.1",
    "@gatsby-mdx-suite/menu": "^0.2.4",
    "@gatsby-mdx-suite/seo": "^0.0.7",
    "@mdx-js/react": "^1.5.5",
    "@theme-ui/preset-tailwind": "^0.3.0",
    "@theme-ui/tailwind": "^0.3.0",
    "@theme-ui/typography": "^0.3.0",
    "esm": "^3.2.25",
    "gatsby": "^2.19.18",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-mdx": "^1.0.73",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-source-contentful": "^2.1.87",
    "gatsby-source-instagram": "^0.6.1",
    "gatsby-source-youtube-v2": "^1.0.1",
    "gatsby-theme-mdx-suite": "^0.7.1",
    "gatsby-transformer-inline-svg": "^0.0.6",
    "gatsby-transformer-sharp": "^2.3.14",
    "gatsby-transformer-sqip": "^2.1.64",
    "gatsby-transformer-video": "^0.1.11",
    "i18next": "^19.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.3.3",
    "theme-ui": "^0.3.1",
    "twin.macro": "^1.0.0-alpha.3",
    "typeface-inter": "^3.12.0",
    "typography-theme-ocean-beach": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-config-standard-react": "^9.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --config .prettierrc",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write --config .prettierrc"
    ]
  }
}
