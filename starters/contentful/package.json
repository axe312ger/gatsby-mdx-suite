{
  "name": "gatsby-starter-mdx-suite-contentful",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.0.33",
  "author": "Benedikt RÃ¶tsch <info@benedikt.roetsch.de>",
  "scripts": {
    "build": "gatsby build",
    "develop": "GATSBY_CONTENTFUL_OFFLINE=true nodemon -x 'gatsby develop' -w './*.js'",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "format": "prettier --write '{./**/*.{js,jsx},*.{js,json}}'",
    "lint": "eslint src",
    "deploy": "gatsby clean && npm run build -- --prefix-paths && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axe312ger/gatsby-suite-mdx"
  },
  "keywords": [
    "gatsby",
    "starter",
    "mdx"
  ],
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@gatsby-mdx-suite/contexts": "^0.0.4",
    "@gatsby-mdx-suite/i18n": "^0.0.11",
    "@gatsby-mdx-suite/mdx-basic": "^0.0.13",
    "@gatsby-mdx-suite/mdx-contentful": "^0.1.7",
    "@gatsby-mdx-suite/mdx-layout": "^0.1.10",
    "@gatsby-mdx-suite/seo": "^0.0.4",
    "@mdx-js/react": "^1.5.5",
    "@theme-ui/typography": "^0.2.46",
    "gatsby": "^2.18.11",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-mdx": "^1.0.67",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-source-contentful": "^2.1.67",
    "gatsby-theme-mdx-suite": "^0.2.2",
    "gatsby-transformer-inline-svg": "^0.0.4",
    "gatsby-transformer-remark": "^2.6.42",
    "gatsby-transformer-sharp": "^2.3.7",
    "gatsby-transformer-sqip": "^2.1.50",
    "i18next": "^19.0.2",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.2.6",
    "simple-icons": "^2.0.0",
    "svg-icon": "^0.8.2",
    "theme-ui": "^0.2.49",
    "typeface-inter": "^3.10.0",
    "typography-theme-grand-view": "^0.16.19",
    "use-deep-compare-effect": "^1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-config-standard-react": "^9.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --config .prettierrc",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write --config .prettierrc",
      "git add"
    ]
  }
}
